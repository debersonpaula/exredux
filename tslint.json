{
  "extends": [
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-react-a11y"
  ],
  "rules": {
    "linebreak-style": false,
    "no-boolean-literal-compare": false,
    "max-line-length": [true, 160],
    "quotemark": [true, "single", "jsx-double"],
    "no-console": true,
    "semicolon": false,
    "variable-name": false
  }
}
